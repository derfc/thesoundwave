<section class="container-fluid">
<h1>home</h1>
     <span class="display-6">
     Welcome to your profile, {{user}}
     </span>
     <img src="{{thumbnail}}" alt="thumbnail" height="60px" width="60px" />
     <p>math.random generating songs/news</p>

<div class="title row">
     <div class="col">
          <p>#</p>
     </div>
     <div class="col">
          <p>SONG</p>
     </div>
     <div class="col">
          <p>ARTIST</p>
     </div>
</div>

{{#each playlistSongArr}}
<div class="album__tracks row">
     <div class="col">
          <p>{{this.id}}</p>
     </div>
     <div class="col">
          <p>{{this.song_name}}</p>
     </div>
     <div class="col">
          <p>{{this.artist_name}}</p>
     </div>
</div>
{{/each}}
</section>
