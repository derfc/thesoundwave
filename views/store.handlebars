<h1>store page</h1>
<section class="mainHomepage">


     <div class="instant-search d-inline mx-3" >
		<input type="search" spellcheck="false" placeholder="Search store" id="filter_store"/>
     </div>
	<div class="sort">
		<label for="sort">Sort By:</label>
			<select name="sort" id="sort">
				<option value="---">---</option>
				<option value="$down">Price High to Low</option>
				<option value="$up">Price Low to High</option>
			</select>
	{{#each category}}
     <button class="btn btn-info button-store search-category" value="{{this}}">{{this}}</button>
	{{/each}}
	</div>


     <div class="greetings text-white">
     </div>

     <div class="results mb-2">
