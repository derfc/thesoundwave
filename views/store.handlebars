<section class="set-height">
     <section class="mainHomepage">

          <div class="instant-search d-inline mx-3">
               <input type="search" spellcheck="false" placeholder="Search store" id="filter_store" />
          </div>
          <div class="sort">
               <label for="sort">Sort By:</label>
               <select name="sort" id="sort">
                    <option value="---">---</option>
                    <option value="$down">Price High to Low</option>
                    <option value="$up">Price Low to High</option>
               </select>
               <button class="btn btn-info button-clear-search clear-search" value="">CLEAR</button>
               {{#each category}}
               <button class="btn btn-info button-store search-category" value="{{this}}">{{this}}</button>
               {{/each}}
          </div>


          <div class="greetings text-white">
          </div>

          <div class="results mb-2">
               <div class="store">
               </div>
               <div class="item"></div>
               {{!-- <div class="song"> </div> --}}
          </div>
          <section class="noneWhenSearch container-fluid px-4">
               <div class="carousel slide" data-ride="carousel">
                    <div class="carousel-inner">
                         <div class="carousel-item active">
                              <div class="row">
                                   <div class="col-sm"><img class="d-block w-100 img-fluid"
                                             src="./image/musicposter1.jpeg" alt="1 slide">
                                   </div>
                                   <div class="col-sm"><img class="d-block w-100 img-fluid"
                                             src="./image/musicposter2.jpeg" alt="2 slide">
                                   </div>
                                   <div class="col-sm"><img class="d-block w-100 img-fluid"
                                             src="./image/musicposter3.jpeg" alt="3 slide">
                                   </div>
                              </div>
                         </div>
                         <div class="carousel-item">
                              <div class="row">
                                   <div class="col-sm"><img class="d-block w-100 img-fluid"
                                             src="./image/musicposter2.jpeg" alt="4 slide">
                                   </div>
                                   <div class="col-sm"><img class="d-block w-100 img-fluid"
                                             src="./image/musicposter3.jpeg" alt="5 slide">
                                   </div>
                                   <div class="col-sm"><img class="d-block w-100 img-fluid"
                                             src="./image/musicposter1.jpeg" alt="6 slide">
                                   </div>
                              </div>
                         </div>
                    </div>
               </div>
          </section>

          {{!-- d-flex flex-wrap flex-fillalbum-card --}}
          <div class="noneWhenSearch homeSong">
               <div class="random-album px-4 row">
                    {{#each item}}
                    <div class="col-lg-2 col-md-3 col-sm-4 my-3">
                         <div class="album-img">
                              <img class="image img-fluid" src="{{this.item_photo}}" alt="Living Things" />
                         </div>
                         <div class="album-info">
                              <h6 class="name mt-2 text-white">{{this.item_name}}</h6>
                         </div>
                         <div><button class="add-to-cart" data-item_id="{{this.id}}" data-user_id="1">add to
                                   cart</button></br></div>
                    </div>
                    {{/each}}
               </div>
          </div>

     </section>

</section>







