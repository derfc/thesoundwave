<section class="set-height">
     <h1>cart page</h1>
     {{#each item}}
     <div class="cart-item">
          <div class="album-img">
               <img class="image img-fluid" src="{{this.item_photo}}" alt="Living Things" />
          </div>
          <p>{{this.item_name}}</p>
          <p class="">${{divide this.item_price 100}}</p>

          <input type="number" class="quantity" min="1" max="100" data-item_id="{{this.id}}" data-user_id="1"
               data-subtotal={{divide this.item_price 100}}>
          <p id="subtotal{{this.id}}" class="subtotal">${{divide this.item_price 100}}</p>
          <button class="delete-item" data-item_id="{{this.id}}" data-user_id="1" data-store_id={{this.store_id}}>delete
               item</button>
     </div>
     {{/each}}
     <p id="total">TOTAL = $</p>
     </br>
     <button id="checkout" class="button-checkout" value="checkout">checkout</button>
     <script src="./script.js"></script>
</section>